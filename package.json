{
  "name": "linter-sass-lint",
  "main": "./lib/main",
  "version": "0.5.0",
  "description": "Atom Linter plugin - Lint your Sass/SCSS with pure node sass-lint",
  "repository": "https://github.com/AtomLinter/linter-sass-lint",
  "license": "MIT",
  "keywords": [
    "Sass",
    "scss",
    "sass-lint",
    "node-sass",
    "linter"
  ],
  "scripts": {
    "lint": "coffeelint ./lib/main.coffee"
  },
  "engines": {
    "atom": ">=1.0.0"
  },
  "dependencies": {
    "atom-linter": "^4.1.1",
    "atom-package-deps": "^3.0.6",
    "sass-lint": "^1.4.0"
  },
  "package-deps": [
    "linter"
  ],
  "providedServices": {
    "linter": {
      "versions": {
        "1.1.0": "provideLinter"
      }
    }
  },
  "devDependencies": {
    "coffeelint": "^1.14.2"
  }
}
